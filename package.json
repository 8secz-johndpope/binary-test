{
  "name": "binary-test",
  "version": "1.3.3",
  "description": "A test app build with Loopback 4 with JWT & Media Upload",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": ">=10.16"
  },
  "author": "IBM Corp.",
  "copyright.owner": "IBM Corp.",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "lb-tsc",
    "build:watch": "lb-tsc --watch",
    "clean": "lb-clean *binary-test-jwt*.tgz dist *.tsbuildinfo package",
    "lint": "npm run prettier:check && npm run eslint",
    "lint:fix": "npm run eslint:fix && npm run prettier:fix",
    "prettier:cli": "lb-prettier \"**/*.ts\"",
    "prettier:check": "npm run prettier:cli -- -l",
    "prettier:fix": "npm run prettier:cli -- --write",
    "eslint": "lb-eslint --report-unused-disable-directives .",
    "eslint:fix": "npm run eslint -- --fix",
    "pretest": "npm run build",
    "test": "lb-mocha \"dist/__tests__/**/*.js\"",
    "test:dev": "lb-mocha --allow-console-logs dist/__tests__/**/*.js && npm run posttest",
    "verify": "npm pack && tar xf loopback-todo*.tgz && tree package && npm run clean",
    "migrate": "node ./dist/migrate",
    "openapi-spec": "node ./dist/openapi-spec",
    "start": "node ."
  },
  "dependencies": {
    "@loopback/authentication": "^7.0.0",
    "@loopback/authentication-jwt": "^0.7.0",
    "@loopback/boot": "^3.0.0",
    "@loopback/core": "^2.10.0",
    "@loopback/repository": "^3.0.0",
    "@loopback/rest": "^7.0.0",
    "@loopback/rest-explorer": "^3.0.0",
    "@loopback/security": "^0.3.0",
    "@loopback/service-proxy": "^3.0.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/bluebird": "^3.5.32",
    "@types/rimraf": "^3.0.0",
    "@types/stoppable": "^1.1.0",
    "@types/toposort": "^2.0.3",
    "@types/uuid": "^8.3.0",
    "bcryptjs": "^2.4.3",
    "bson": "^4.1.0",
    "js-yaml": "^3.14.0",
    "loopback-connector-mysql": "^6.0.0",
    "loopback-connector-rest": "^3.7.0",
    "multer": "^1.4.2",
    "tslib": "^2.0.1"
  },
  "devDependencies": {
    "@loopback/build": "^6.2.3",
    "@loopback/eslint-config": "^10.0.0",
    "@loopback/http-caching-proxy": "^2.1.14",
    "@types/express-serve-static-core": "^4.17.12",
    "@types/multer": "^1.4.4",
    "@loopback/testlab": "^3.2.5",
    "@types/lodash": "^4.14.161",
    "@types/node": "^10.17.35",
    "eslint": "^7.9.0",
    "lodash": "^4.17.20",
    "typescript": "~4.0.2"
  },
  "keywords": [
    "loopback",
    "LoopBack",
    "example",
    "tutorial",
    "CRUD",
    "models",
    "binary",
    "jwt",
    "authentication"
  ]
}
